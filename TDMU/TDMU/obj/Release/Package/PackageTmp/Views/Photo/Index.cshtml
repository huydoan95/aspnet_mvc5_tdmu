        ViewBag.Title = "Our Photo - Thu Dau Mot University";
        Layout = "~/Views/Shared/_OurPhoto.cshtml";
    }
    @section Menu{
        <li><a href="/home" title="Home">home</a></li>
        <li><a href="/about" title="About">About us</a></li>
        <li><a href="/news" title="News">news</a></li>
        <li><a href="/trainingologies" title="TRAINING OLOGIES">TRAINING OLOGIES</a></li>
