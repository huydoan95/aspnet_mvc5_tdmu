@model List<Model.EF.Photo>

<div class="col-xs-12 col-sm-6 col-md-12 col-lg-12">
    <section>
        <div class="section-title">
            <h4><a href="/ourphoto" style="color:#B1273E;">our photos</a></h4>
        </div>

        <div class="row our-photos">
            @foreach (var item in Model)
            {
                <div class="photo-item col-xs-4 col-sm-4 col-md-4 col-lg-4">
                    <a href="@item.Image" class="swipebox" title="@item.Name">
                        <img src="@item.Image" alt="image" style="width:128px;height:128px;">
                    </a>
                </div>
            }
            
        </div>
    </section>
</div>
